#include <bits/stdc++.h>
using namespace std;
 
int main(){
    int n, s1, s2;
    set<int> k;
    set<int> q;
    
    cin >> n;  
    
    for(int i = 0; i < n; i++){
        cin >> s1 >> s2;  
        k.insert(s1);   
        q.insert(s2);    
    }   
    
    if(k.size() <= q.size()){
        cout << k.size();
    } else {
        cout << q.size();
    }
    
    return 0;
}
