#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n;
    cin >> n;
 
    vector<char> s(n);
 
    for (int i = 0; i < n; i++) {
        cin >> s[i];
    }
 
    int c = 0, d = 0;
    for (int i = 0; i < n; i++) {
        if (s[i] == 'A') {
            c++;
        } else {
            d++;
        }
    }
 
    if (c > d) {
        cout << "Anton";
    } else if (c < d) {
        cout << "Danik";
    } else {
        cout << "Friendship";
    }
cin.get();
cin.get();
return 0;
}
